	</dependency> -->
	<!-- log4j-api  如果想把log4j打印的日志重定向到slf4j，就不能引入log4j的包，取而代之的是log4j-api包-->
	<dependency>
	    <groupId>org.apache.logging.log4j</groupId>
	    <artifactId>log4j-api</artifactId>
	    <version>2.9.1</version>
	</dependency>
	
	<!-- log4j2 -->
